{
  "name": "adofai-gg",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "node prebuild.cjs && vite build",
    "package": "vite package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync && husky install",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "tw": "tailwind-config-viewer --config tailwind.config.cjs"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@playwright/test": "^1.21.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/fs-extra": "^9.0.13",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.5",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.1",
    "tailwind-config-viewer": "^1.7.0",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "unzipper": "^0.10.11",
    "vite": "^3.0.0",
    "vite-plugin-glob": "^0.2.7"
  },
  "type": "module",
  "dependencies": {
    "@adofai-gg/svelte-virtualized-infinite-scroll": "^1.0.9",
    "@sentry/browser": "^7.5.0",
    "@sentry/integrations": "^7.7.0",
    "@sveltejs/adapter-node": "^1.0.0-next.73",
    "clsx": "^1.1.1",
    "simplebar": "^5.3.8",
    "svelte-icons": "^2.1.0",
    "tippy.js": "^6.3.7"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,svelte}": "eslint --cache --fix",
    "*.{js,ts,jsx,tsx,svelte,css,scss,md}": "prettier --ignore-path .gitignore --write"
  }
}
